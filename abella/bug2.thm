Specification "ho".

Theorem bad :
    forall (T : val -> expr) (E : expr -> val -> expr) D,
    nabla n1 n2,
    (forall (E : expr -> expr) T (D : expr), nabla (k : expr) x, E k = D -> T x = D -> E (let k T) = D)
    -> T n2 = D
    -> E n1 n2 = D
    -> false.
  intros.
  apply H1 to H3 H2.
